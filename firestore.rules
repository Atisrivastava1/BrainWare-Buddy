rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // Allow read access to the knowledge_base collection
    match /knowledge_base/{docId} {
      allow read: if true;
      allow write: if false; // Protect your data from being written by clients
    }

    match /{document=**} {
      allow read, write: if false;
    }
  }
}
